{
  "name": "root",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:prod": "NODE_ENV=production npx lerna run build",
    "tsc": "NODE_ENV=production npx lerna exec -- tsc",
    "lint": "prettier -c . && npx lerna exec -- eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier -w . && npx lerna exec -- eslint . --ext .ts,.tsx,.js,.jsx",
    "build:dev": "NODE_ENV=development npx lerna run build",
    "publish:prod": "NODE_ENV=production npx lerna publish"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@oboku/shared": "file:../shared",
    "@types/jest": "^27.0.3",
    "@types/request": "^2.48.8",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "cheerio": "^1.0.0-rc.10",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.5",
    "jest": "^28.0.3",
    "lerna": "^4.0.0",
    "nano": "^10.0.0",
    "node-fetch": "^3.2.4",
    "prettier": "^2.6.2",
    "request": "^2.88.2",
    "ts-loader": "^9.3.0",
    "url-loader": "4.1.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "yup": "^0.32.11"
  }
}
